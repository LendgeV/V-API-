    <script>
        document.addEventListener("DOMContentLoaded", function() {
            const cards = document.querySelectorAll(".motion-card");
            
            // Using Framer Motion's animate function with stagger
            motion.animate(
                cards,
                {
                    opacity: [0, 1],
                    y: [20, 0]
                },
                {
                    duration: 0.5,
                    delay: motion.stagger(0.075),
                    ease: "easeOut"
                }
            );
        });
    </script>
